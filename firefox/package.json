{
  "name": "mdp2p",
  "version": "5.1",
  "description": "P2P Browser Extensions",
  "scripts": {
    "buildev": "webpack --config=./webpack.config.js --mode development --env=development",
    "buildprod": "webpack --config=./webpack.config.js --mode production --env=production",
    "start": "webpack-dev-server --open",
    "build": "parcel build src/manifest.json --experimental-scope-hoisting --no-source-maps --public-url ./",
    "watch": "parcel watch src/manifest.json --public-url ./"
  },
  "keywords": [
    "P2P"
  ],
  "author": "ragonzalez@disroot.org",
  "license": "MIT",
  "devDependencies": {
    "@types/bootstrap": "^5.0.0",
    "@types/jquery": "^3.5.4",
    "bootstrap": "^4.5.3",
    "jquery": "^3.5.1",
    "parcel-plugin-web-extension": "^1.6.1",
    "typescript": "^4.0.5",
    "web-ext-types": "^3.2.1",
    "webpack": "^2.3.2"
  },
  "dependencies": {
    "eslint": "^7.13.0",
    "eslint-config-xtrict": "^3.0.1",
    "javascript-obfuscator": "^2.4.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-web-ext-tool": "^0.2.0",
    "web-ext": "^5.3.0",
    "webextension-polyfill": "^0.6.0",
    "webextension-polyfill-ts": "^0.22.0"
  }
}
